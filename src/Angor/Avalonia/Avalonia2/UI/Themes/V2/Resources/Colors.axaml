    <!-- ═══════════════════════════════════════════════════════════════════════
         SHARED RESOURCES (theme-independent)
         ═══════════════════════════════════════════════════════════════════════ -->

    <!-- ── Stepper Colors (same in both themes) ── -->
    <SolidColorBrush x:Key="StepCompletedGreen" Color="#2D5A3D" />
    <SolidColorBrush x:Key="StepCurrentGreen" Color="#4B7C5A" />
    <SolidColorBrush x:Key="StepInactiveGray" Color="#888888" />
    <SolidColorBrush x:Key="StepLineGreen" Color="#2D5A3D" />

    <!--
        App-wide crosshatch texture replicating the web CSS:
          repeating-linear-gradient(45deg, transparent 2px, rgba(255,255,255,.02) 4px)
          repeating-linear-gradient(-45deg, transparent 2px, rgba(0,0,0,.01) 4px)
        Applied as a full-span non-interactive overlay over the entire ShellView.
    -->
    <DrawingBrush x:Key="AppTextureBrush" TileMode="Tile" DestinationRect="0,0,4,4" Stretch="None">
        <DrawingBrush.Drawing>
            <DrawingGroup>
                <GeometryDrawing>
                    <GeometryDrawing.Pen>
                        <Pen Brush="#05FFFFFF" Thickness="1" />
                    </GeometryDrawing.Pen>
                    <GeometryDrawing.Geometry>
                        <GeometryGroup>
                            <LineGeometry StartPoint="0,2" EndPoint="2,0" />
                            <LineGeometry StartPoint="2,4" EndPoint="4,2" />
                        </GeometryGroup>
                    </GeometryDrawing.Geometry>
                </GeometryDrawing>
                <GeometryDrawing>
                    <GeometryDrawing.Pen>
                        <Pen Brush="#03000000" Thickness="1" />
                    </GeometryDrawing.Pen>
                    <GeometryDrawing.Geometry>
                        <GeometryGroup>
                            <LineGeometry StartPoint="2,0" EndPoint="4,2" />
                            <LineGeometry StartPoint="0,2" EndPoint="2,4" />
                        </GeometryGroup>
                    </GeometryDrawing.Geometry>
                </GeometryDrawing>
            </DrawingGroup>
        </DrawingBrush.Drawing>
    </DrawingBrush>
    
    <!-- CheckBox overrides -->
    <StaticResource x:Key="CheckBoxCheckBackgroundFillChecked" ResourceKey="Accent" />
