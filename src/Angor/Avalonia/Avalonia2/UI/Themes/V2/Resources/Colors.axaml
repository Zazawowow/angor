    <!-- Green gradient for small action buttons (Vue: linear-gradient(135deg, #4b7c5a, #3d6448)) -->
    <LinearGradientBrush x:Key="GreenGradient" StartPoint="0%,0%" EndPoint="100%,100%">
        <GradientStop Color="#4B7C5A" Offset="0" />
        <GradientStop Color="#3D6448" Offset="1" />
