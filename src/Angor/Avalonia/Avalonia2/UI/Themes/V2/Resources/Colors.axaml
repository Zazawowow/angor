    <SolidColorBrush x:Key="AccentButtonForeground" Color="{DynamicResource SurfaceHover}" />
    <SolidColorBrush x:Key="AccentButtonBackground" Color="{DynamicResource Accent}" />
    <SolidColorBrush x:Key="AccentButtonBorder" Color="{DynamicResource Accent}" />

    <!-- Brand Wash - subtle green brand tint -->
    <LinearGradientBrush x:Key="BrandWash" StartPoint="0%,0%" EndPoint="100%,100%">
        <GradientStop Color="#192D5A3D" Offset="0" />
        <GradientStop Color="#102D5A3D" Offset="1" />
